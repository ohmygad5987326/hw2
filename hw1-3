// function statement
function greet() {
	console.log('hi');
}
greet();

// function expression
var greetMe = function() {
	console.log('Hi Tony!');
}
greetMe();

// arrow function expression
greet = () =>console.log('hi');
greet();
// arrow function expression with parameters
greetMe = name =>console.log('Hi'+name+"!");
greetMe("Tony");
